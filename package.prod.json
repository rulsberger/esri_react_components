{
  "name": "@piercecounty/react_components",
  "version": "1.0.0",
  "description": "Reusable components for GIS applications",
  "main": "dist/IdentifyAllWidget.cjs.js",
  "module": "dist/IdentifyAllWidget.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/IdentifyAllWidget.es.js",
      "require": "./dist/IdentifyAllWidget.cjs.js"
    },
    "./DrawWidget": {
      "import": "./dist/DrawWidget.es.js",
      "require": "./dist/DrawWidget.cjs.js"
    },
    "./FeatureListWidget": {
      "import": "./dist/FeatureListWidget.es.js",
      "require": "./dist/FeatureListWidget.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --config vite.test.config.ts"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "@arcgis/core": "4.31.6",
    "@esri/calcite-components-react": "^2.13.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vitest": "^3.0.6"
  },
  "publishConfig": {
    "registry": "https://api.bitbucket.org/2.0/repositories/piercecountywa-ss/react_components/npm/"
  }
